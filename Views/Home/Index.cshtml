@model IEnumerable<Movie>
<div class="row">
    <div class="col-md-2 mb-4">
        @*@Html.Partial("_Menu",Model.Categories)*@
        @await Component.InvokeAsync("CategoryMenu")
    </div>
    <div class="col-md-10">
        @if (Model.Count()==0)
        {
            @Html.Partial("_Nofilm" )
        }
        else
        {
            @Html.Partial("_Movies" , Model)
        }
        
    </div>
</div>
